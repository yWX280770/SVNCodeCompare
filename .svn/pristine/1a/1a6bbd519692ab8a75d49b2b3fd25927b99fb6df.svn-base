package com.chinasoft.model;

public enum Action 
{

	//修改
	M(0),
	
	//新增
	A(1),
	
	//delete
	D(2),
	
	//未知
	UNKNOWN(-1);
	
	int value = -1;
	
	private Action(int i) 
	{
		this.value = i;
	}

	public static Action parseAction(String act) 
	{
		switch(act)
		{
			case "M":
				return M;
			case "A":
				return A;
			case "D":
				return D;
			default:
				return UNKNOWN;
		}
	}
}
